# mount point	fstype		device			device2			flags
/system_image		emmc	/dev/block/platform/bootdevice/by-name/system		flags=backup=0;flashimg;display="System Image"
/vendor_image		emmc	/dev/block/platform/bootdevice/by-name/vendor		flags=backup=0;flashimg;display="Vendor Image"
/product_image		emmc	/dev/block/platform/bootdevice/by-name/product		flags=backup=0;flashimg;display="Product Image"
/odm_image		emmc	/dev/block/platform/bootdevice/by-name/odm		flags=backup=0;flashimg;display="Odm Image"
/system_ext_image	emmc	/dev/block/platform/bootdevice/by-name/system_ext	flags=backup=0;flashimg;display="System_EXT Image"
/vendor_dlkm_image	emmc	/dev/block/platform/bootdevice/by-name/vendor_dlkm      flags=backup=0;flashimg;display="Vendor_DLKM Image"

/vbmeta           emmc    /dev/block/platform/bootdevice/by-name/vbmeta           flags=backup=1;display="VBMeta";flashimg=1

/data             f2fs    /dev/block/platform/bootdevice/by-name/userdata         fileencryption=aes-256-xts:aes-256-cts:v2+inlinecrypt_optimized,keydirectory=/metadata/vold/metadata_encryption,metadata_encryption=aes-256-xts
/metadata         ext4    /dev/block/platform/bootdevice/by-name/metadata         flags=display="Metadata"


/protect_f        ext4    /dev/block/platform/bootdevice/by-name/protect1                 flags=display="Protect_F";backup=1;
/protect_s        ext4   /dev/block/platform/bootdevice/by-name/protect2                 flags=display="Protect_S";backup=1;
/persist          ext4    /dev/block/platform/bootdevice/by-name/persist                  flags=display="Persist";backup=1;
/nvram            emmc   /dev/block/platform/bootdevice/by-name/nvram       flag=display="Nvram";backup=1
/nvdata           ext4   /dev/block/platform/bootdevice/by-name/nvdata      flags=display="NvData";backup=1;subpartitionof=/nvram;
/nvcfg            ext4   /dev/block/platform/bootdevice/by-name/nvcfg       flags=display="NvCfg";backup=1;subpartitionof=/nvram;
/persistent       emmc   /dev/block/platform/bootdevice/by-name/frp
/proinfo          emmc   /dev/block/platform/bootdevice/by-name/proinfo
/para             emmc   /dev/block/platform/bootdevice/by-name/para
/logo             emmc   /dev/block/platform/bootdevice/by-name/logo

#MICRO SD_CARD
/external_sd       	vfat    /dev/block/mmcblk1p1 /dev/block/mmcblk1		        flags=storage;wipeingui;removable

#USB-OTG
/usb-otg         	auto    /dev/block/sda1 /dev/block/sda      	   	        flags=display="USB-OTG";storage;wipeingui;removable
